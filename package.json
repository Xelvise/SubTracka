{
    "name": "subscription-tracker",
    "version": "1.0.0",
    "description": "a Subscription tracker",
    "main": "dist/app.js",
    "scripts": {
        "dev": "NODE_ENV=dev NODE_OPTIONS='--no-deprecation=DEP0040' nodemon --exec tsx app.ts",
        "prod": "tsc && NODE_ENV=prod node dist/app.js",
        "qstash:local": "NODE_OPTIONS='--dns-result-order=ipv4first' npx @upstash/qstash-cli dev --port 8081",
        "docker_compose_up": "mkdir -p pgdata && sudo chown -R $USER:$USER pgdata && sudo chmod -R 777 pgdata && docker compose --env-file .env.dev up",
        "introspect_db": "npx drizzle-kit pull",
        "generate_migration": "npx drizzle-kit generate",
        "migrate_schema": "npx drizzle-kit migrate",
        "push_migration": "npx drizzle-kit push",
        "create_custom_migration": "npx drizzle-kit generate --custom"
    },
    "author": "Elvis Gideon",
    "license": "ISC",
    "dependencies": {
        "@upstash/ratelimit": "^2.0.5",
        "@upstash/redis": "^1.35.1",
        "@upstash/workflow": "^0.2.16",
        "bcrypt": "^6.0.0",
        "cookie-parser": "^1.4.7",
        "dayjs": "^1.11.13",
        "dotenv": "^17.0.1",
        "drizzle-orm": "^0.44.2",
        "express": "^5.1.0",
        "express-rate-limit": "^8.0.1",
        "express-validator": "^7.2.1",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^7.0.5",
        "postgres": "^3.4.7",
        "serverless-http": "^3.2.0"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/cookie-parser": "^1.4.9",
        "@types/dotenv": "^6.1.1",
        "@types/express": "^5.0.3",
        "@types/express-rate-limit": "^5.1.3",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/node": "^24.0.10",
        "@types/nodemailer": "^6.4.17",
        "drizzle-kit": "^0.31.4",
        "nodemon": "^3.1.10",
        "serverless-plugin-typescript": "^2.1.5",
        "supabase": "^2.30.4",
        "tsx": "^4.20.3",
        "typescript": "^5.8.3"
    }
}
